<?xml version="1.0"?>
<CREOLE-DIRECTORY>
    <CREOLE>
        <RESOURCE>
            <!-- Name of the PR that appears in GATE PR List -->
            <NAME>Malparser</NAME>
            <!-- Jar where to look for the resource -->
            <JAR>maltparser-plugin.jar</JAR>
            <JAR>lib/maltparser-1.7.2.jar</JAR>
            <JAR>lib/liblinear-1.8.jar</JAR>
            <JAR>lib/libsvm.jar</JAR>
            <JAR>lib/log4j.jar</JAR>
            <!-- Underlying class that implements the New Plugin -->
            <CLASS>vn.com.epi.gate.maltparser.MaltParserPlugin</CLASS>
            <!-- Comment that appears when mouse hovers over the PR Name -->
            <PARAMETER COMMENT="The document to be processed"
                       NAME="document" RUNTIME="true">
                gate.Document
            </PARAMETER>
            <PARAMETER COMMENT="The document to be processed"
                       NAME="document" RUNTIME="true">
                gate.Document
            </PARAMETER>
            <PARAMETER COMMENT="Directory of .mco file"
                       NAME="dirOfMCO" RUNTIME="false" DEFAULT="resources">
                java.net.URL
            </PARAMETER>
            <PARAMETER COMMENT="Name of .mco file"
                       NAME="nameOfMCO" RUNTIME="false" DEFAULT="VTB.mco">
                java.lang.String
            </PARAMETER>
        </RESOURCE>
    </CREOLE>
</CREOLE-DIRECTORY>
