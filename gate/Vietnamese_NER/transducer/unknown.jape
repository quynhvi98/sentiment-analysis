Rule: ProductUnknown
Priority: 10
(
	({Token.orth == upperInitial, Token.category =~ "Np?"} 
		| {Token.orth == allCaps} 
		| {Token.orth == mixedCaps})
	(
		(
			(
				(
					({Token.kind == number})
					({Token.orth == upperInitial, Token.category =~ "Np?"} 
						| {Token.orth == allCaps} 
						| {Token.orth == mixedCaps})
				)[0, 5]
				({Token.kind == number})? 
			)|
			(
				({Token.kind == number})?
				(
					({Token.string == "."})
					({Token.kind == number})
				)[0, 5]
			)
		) |
	    ({Token.orth == upperInitial, Token.category =~ "Np?"} 
			| {Token.orth == allCaps} 
			| {Token.orth == mixedCaps})[0, 5]
	)
):unknown
-->
